<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tms.figuresfx.controller.MainScreenViewController">
   <children>
      <Button mnemonicParsing="false" onMouseClicked="#ellipseButton" text="Ellipse" />
      <Button mnemonicParsing="false" onMouseClicked="#roundrectButton" text="Round Rectangle" />
      <Button mnemonicParsing="false" onMouseClicked="#rectangleButton" text="Rectangle" />
      <Button mnemonicParsing="false" onMouseClicked="#circleButton" text="Circle" />
      <Button mnemonicParsing="false" onMouseClicked="#triangleButton" text="Triangle" />
      <Button mnemonicParsing="false" onMouseClicked="#clearButton" text="Clear" />
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="1024.0">
         <children>
            <Canvas fx:id="canvas" height="600.0" layoutX="53.0" layoutY="60.0" onMouseClicked="#onMouseClicked" width="1024.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
   </children>
</FlowPane>
